{% extends 'base.html.twig' %}

{% block title %}Nuevo contacto{% endblock %}

{% block body %}

    <h1>Nuevo contacto</h1>

    {{ form_start(formulario, {'attr': {'novalidate': 'novalidate'}}) }}

        {{ form_row(formulario.nombre) }}

        {{ form_row(formulario.telefono) }}

        {{ form_row(formulario.email) }}

        {{ form_row(formulario.provincia) }}

    {{ form_end(formulario) }}

{% endblock %}
